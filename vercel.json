{
  "version": 2,
  "builds": [
    {
      "src": "deployforge/wsgi.py",
      "use": "@vercel/python"
    },
    { "src": "staticfiles/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/staticfiles/$1" },
    {
      "src": "/(.*)",
      "dest": "deployforge/wsgi.py"
    }
  ]
}
